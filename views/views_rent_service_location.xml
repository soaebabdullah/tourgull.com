<odoo>
    <data>


        <!-- tree - rent.car.service_location -->
        <!-- tree - rent_car_service_location -->
        <record model="ir.ui.view" id="rent_car_service_location_tree_id">
            <field name="name">rent_car_service_location_tree</field>
            <field name="model">rent.car.service_location</field>
            <field name="arch" type="xml">
                <tree default_order="id desc">
                    <field name="name"/>
                </tree>
            </field>
        </record>


        <!-- form - host.applications -->
        <record model="ir.ui.view" id="rent_car_service_location_form_id">
            <field name="name">rent_car_service_location_form</field>
            <field name="model">rent.car.service_location</field>
            <field name="arch" type="xml">
                <form default_order="id desc">
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                            </group>
                            <group></group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


        <!-- actions opening views on models -->
        <record model="ir.actions.act_window" id="service_location_action">
            <field name="name">Service Location</field>
            <field name="res_model">rent.car.service_location</field>
            <field name="view_mode">tree,form</field>
        </record>



    </data>
</odoo>