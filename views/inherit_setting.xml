<odoo>
    <record id="res_config_settings_view_form2" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.website.sale2</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="website.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <div id="sale_checkout_settings" position="after">
                <h2>Available share</h2>

                <div class="row mt16 o_settings_container" id="sale_product_catalog_settings2">
                    <div class="col-12 col-lg-6 o_setting_box" id="product_attributes_setting2">
                        <div class="o_setting_left_pane"></div>
                        <div class="o_setting_right_pane">
                            <div class="content-group">
                                <div class="row mt16">
                                    <label class="o_light_label col-lg-4" string="Temporary booking time (Seconds)" for="rp_temp_book_time"/>
                                    <field name="rp_temp_book_time"/>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row mt16 o_settings_container" id="rp_av_commission1">
                    <div class="col-12 col-lg-6 o_setting_box" id="rp_av_commission2">
                        <div class="o_setting_left_pane"></div>
                        <div class="o_setting_right_pane">
                            <div class="content-group">
                                <div class="row mt16">
                                    <label class="o_light_label col-lg-4" string="Av Commission" for="rp_av_commission"/>
                                    <field name="rp_av_commission"/>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </field>
    </record>
</odoo>