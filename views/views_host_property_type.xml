<odoo>
    <data>


        <!-- tree - host.property_type -->
        <!-- tree - host_property_type -->
        <record model="ir.ui.view" id="host_property_type_tree_id">
            <field name="name">host_property_type_tree</field>
            <field name="model">host.property_type</field>
            <field name="arch" type="xml">
                <tree default_order="id desc">
                    <field name="name"/>
                </tree>
            </field>
        </record>


        <!-- form -->
        <record model="ir.ui.view" id="host_property_type_form_id">
            <field name="name">host_property_type_tree</field>
            <field name="model">host.property_type</field>
            <field name="arch" type="xml">
                <form default_order="id desc">
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                            </group>
                            <group></group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


        <!-- actions -->
        <record model="ir.actions.act_window" id="host_property_type_action">
            <field name="name">Host Property Type</field>
            <field name="res_model">host.property_type</field>
            <field name="view_mode">tree,form</field>
        </record>




    </data>
</odoo>