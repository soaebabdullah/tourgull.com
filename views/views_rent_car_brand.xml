<odoo>
    <data>


        <!-- tree - rent.car.brand -->
        <record model="ir.ui.view" id="rent_car_brand_tree_id">
            <field name="name">rent_car_brand_tree</field>
            <field name="model">rent.car.brand</field>
            <field name="arch" type="xml">
                <tree default_order="id desc">
                    <field name="name"/>
                </tree>
            </field>
        </record>


        <!-- form - rent.car.app -->
        <record model="ir.ui.view" id="rent_car_brand_form_id">
            <field name="name">rent_car_brand_form</field>
            <field name="model">rent.car.brand</field>
            <field name="arch" type="xml">
                <form default_order="id desc">
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                            </group>
                            <group></group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


        <!-- actions opening views on models -->
        <record model="ir.actions.act_window" id="car_brand_action">
            <field name="name">Car Brand</field>
            <field name="res_model">rent.car.brand</field>
            <field name="view_mode">tree,form</field>
        </record>



    </data>
</odoo>